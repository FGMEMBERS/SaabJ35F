<?xml version="1.0"?>

<PropertyList>

  <path>navradio.ac</path>

 <animation>
  <type>material</type> 
  <object-name>Selector</object-name>
  <object-name>Anita1</object-name>
  <object-name>Anita2</object-name>
  <object-name>Barbro</object-name>
  <object-name>Nav1</object-name>
  <object-name>Nav2</object-name>
  <object-name>Landn</object-name>
  <object-name>Cube</object-name>
  <condition>		
    <greater-than>		
     <property>/sim/time/sun-angle-rad</property>
     <value>1.57</value>
    </greater-than>
  </condition>
  <emission>	
   <red>0.15</red>	
   <green>0.03</green>	
   <blue>0.03</blue>
   <factor-prop>controls/lighting/instruments-norm</factor-prop>
  </emission>
 </animation>
  
<!-- PICK -->

 <animation>
  <type>pick</type>
   <object-name>Anita1R</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
      <command>property-cycle</command>
      <property>/instrumentation/navradio/anita1</property>
      <value type="int">0</value>
      <value type="int">1</value>
      <value type="int">2</value>
      <value type="int">3</value>
      <value type="int">4</value>
      <value type="int">5</value>
      <value type="int">6</value>
      <value type="int">7</value>
      <value type="int">8</value>
      <value type="int">9</value>
      <value type="int">10</value>
      <value type="int">11</value>
      <value type="int">12</value>
      <value type="int">13</value>
      <value type="int">14</value>
      <value type="int">15</value>
   </binding>
   <mod-up>
     <binding>
       <command>nasal</command>
       <script>SaabJ35F.set_beacon();</script>
     </binding>
   </mod-up>
   </action>
 </animation>
 

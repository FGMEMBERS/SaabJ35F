 <aerodynamics>


        <function name="aero/function/kCLge">
            <description>Change_in_lift_due_to_ground_effect</description>
            <product>
                  <table>
                      <independentVar>aero/h_b-mac-ft</independentVar>
                      <tableData>
                          0.0000  1.2290
                          0.1000  1.2040
                          0.1500  1.1460
                          0.2000  1.1240
                          0.3000  1.1050
                          0.4000  1.0410
                          0.5000  1.0340
                          0.6000  1.0190
                          0.7000  1.0080
                          0.8000  1.0030
                          0.9000  1.0010
                          1.0000  1.0000
                          1.1000  1.0000
                      </tableData>
                  </table>
            </product>
        </function>

        <function name="aero/function/CA-1">
           <description>Quotient of inlet velocit and air velocity minus one</description>
           <min>
             <sum>
               <quotient>
                 <product>
                   <property>propulsion/engine/n1</property>
                   <value>4</value> <!-- Approx 400 ft/s at max RPM -->
                 </product>
                 <property>velocities/u-aero-fps</property>
               </quotient>
               <value>-1.0</value>
             </sum>
             <value>9</value> <!-- Avoids unrealistic effects at very low speeds  -->
           </min>
         </function>


<?xml version="1.0"?>
<PropertyList>
  <path>draken_AHK.ac</path>

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>10</max-m>
 </animation>

 <animation>
  <type>material</type> 
  <object-name>Instrument</object-name>
  <condition>		
    <greater-than>		
     <property>/sim/time/sun-angle-rad</property>
     <value>1.57</value>
    </greater-than>
  </condition>
  <emission>	
   <red>0.15</red>	
   <green>0.03</green>	
   <blue>0.03</blue>
   <factor-prop>controls/lighting/instruments-norm</factor-prop>
  </emission>
 </animation>
  
 <animation>
  <type>material</type> 
  <object-name>Needle_dist</object-name>
  <object-name>Needle_h1000</object-name>
  <object-name>Needle_h100</object-name>
  <object-name>Needle_target</object-name>
  
  <object-name>Command</object-name>
  <object-name>Face</object-name>
  <emission>	
   <red>1</red>	
   <green>0.3</green>	
   <blue>0.3</blue>
   <factor-prop>controls/lighting/instruments-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>Needle_dist</object-name>
  <property>/instrumentation/AHK/needle_dist</property>
  <interpolation>
   <entry><ind>0</ind><dep>-0.043</dep></entry>
   <entry><ind>1</ind><dep>-0.5</dep></entry>
  </interpolation>
  <axis>
   <x>1</x>
   <y>0</y>
  </axis>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>Needle_h1000</object-name>
  <property>instrumentation/altimeter/pressure-alt-ft</property>
  <interpolation>
   <entry><ind>0.0</ind><dep>-0.043</dep></entry>
   <entry><ind>67213</ind><dep>-0.5</dep></entry>
  </interpolation>
  <axis>
   <x>1</x>
   <y>0</y>
  </axis>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>Needle_h100</object-name>
  <property>instrumentation/altimeter/pressure-alt-ft</property>
  <interpolation>
   <entry><ind>-328</ind><dep>-0.007</dep></entry>
   <entry><ind>0.0</ind><dep>0.0</dep></entry>
   <entry><ind>6721</ind><dep>0.1423</dep></entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>Needle_target</object-name>
  <property>/instrumentation/AHK/needle_target</property>
  <interpolation>
   <entry><ind>0.0</ind><dep>0.0</dep></entry>
   <entry><ind>1.0</ind><dep>0.1423</dep></entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>Command</object-name>
  <property>/instrumentation/AHK/mode</property>
  <factor>-0.09375</factor>
  <axis>
   <x>0</x>
   <y>1</y>
  </axis>
 </animation>

</PropertyList>

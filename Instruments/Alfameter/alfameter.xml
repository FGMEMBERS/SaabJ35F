<?xml version="1.0"?>
<PropertyList>
  <path>alfameter.ac</path>

<animation>
        <type>rotate</type>
        <object-name>Needle</object-name>
        <property>orientation/alpha-deg</property>
        <interpolation>
            <entry><ind>-7</ind><dep>-80</dep></entry>
            <entry><ind>30</ind><dep>205</dep></entry>
        </interpolation>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
</animation>

</PropertyList>

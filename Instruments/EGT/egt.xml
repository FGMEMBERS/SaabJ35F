<?xml version="1.0"?>
<PropertyList>
  <path>egt.ac</path>

<animation>
        <type>rotate</type>
        <object-name>Needle</object-name>
        <property>engines/engine/egt-degf</property>
        <interpolation>
            <entry><ind>122</ind><dep>-13.5</dep></entry>
            <entry><ind>1544</ind><dep>199.8</dep></entry>
        </interpolation>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
</animation>

</PropertyList>

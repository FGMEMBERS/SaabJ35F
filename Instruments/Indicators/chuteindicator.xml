<?xml version="1.0"?>

<PropertyList>
  <path>chuteindicator.ac</path>

<animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>10</max-m>
 </animation>
 
 <animation>
  <type>material</type> 
  <object-name>Chute_indicator</object-name>
  <emission>	
   <red>0.3</red>	
   <green>0.3</green>	
   <blue>0.3</blue>
   <factor-prop>controls/electric/battery-switch</factor-prop>
  </emission>
 </animation>
 
 <animation>
  <type>textranslate</type>
  <object-name>Chute_indicator</object-name>
  <property>/instrumentation/chute_state</property>
  <factor>0.25</factor>
  <axis>
   <x>1</x>
   <y>0</y>
  </axis>
 </animation>

</PropertyList>

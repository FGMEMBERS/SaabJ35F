<?xml version="1.0"?>

<PropertyList>
  <path>gearindicator.ac</path>

 <animation>
  <type>material</type> 
  <object-name>NoseW</object-name>
  <condition>
   <greater-than>
    <property>systems/electrical/outputs/battery</property>
    <value>20</value>
   </greater-than>
  </condition>
  <emission>
   <red>0.0</red>
   <green>1.0</green>
   <blue>0.0</blue>
   <factor-prop>/gear/gear[0]/position-norm</factor-prop>
   <step>1</step>
  </emission>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>NoseW</object-name>
  <condition>
   <greater-than>
    <property>systems/electrical/outputs/battery</property>
    <value>20</value>
   </greater-than>
  </condition>
  <property>/gear/gear[0]/position-norm</property>
  <factor>0.5</factor>
  <step>0.5</step>
  <axis>
   <x>1</x>
   <y>0</y>
  </axis>
 </animation>


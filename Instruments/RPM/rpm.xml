<?xml version="1.0"?>
<PropertyList>
  <path>rpm.ac</path>

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>10</max-m>
 </animation>
 
 <animation>
  <type>material</type> 
  <object-name>Needle</object-name>
  <object-name>Plane</object-name>
  <emission>	
   <red>1</red>	
   <green>0.3</green>	
   <blue>0.3</blue>
   <factor-prop>controls/lighting/instruments-norm</factor-prop>
  </emission>
 </animation>
 
<animation>
        <type>rotate</type>
        <object-name>Needle</object-name>
        <property>engines/engine/n1</property>
        <interpolation>
            <entry><ind>20</ind><dep>0</dep></entry>
            <entry><ind>110</ind><dep>324</dep></entry>
        </interpolation>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
</animation>

</PropertyList>

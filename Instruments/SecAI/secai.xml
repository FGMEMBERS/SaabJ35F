<?xml version="1.0"?>
<PropertyList>

  <path>secai.ac</path>

  <animation>
    <type>rotate</type>
    <object-name>Cylinder</object-name>
    <property>orientation/roll-deg</property>
    <center>
      <x-m>-0.02522</x-m>
      <y-m>0</y-m>
      <z-m>0.0</z-m>
    </center>
    <axis><x>1</x></axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Pointer</object-name>
    <property>orientation/roll-deg</property>
    <center>
      <x-m>-0.02522</x-m>
      <y-m>0</y-m>
      <z-m>0.0</z-m>
    </center>
    <axis><x>1</x></axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>Cylinder</object-name>
    <property>orientation/pitch-deg</property>
    <factor>2</factor>
    <center>
      <x-m>-0.02522</x-m>
      <y-m>0</y-m>
      <z-m>0.0</z-m>
    </center>
    <axis><y>1</y></axis>
  </animation>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>15</max-m>
  </animation>

</PropertyList>

<?xml version="1.0"?>

<PropertyList>

 <path>plastic_pin_switch.ac</path>

 <params>
  <pos>/instrumentation/switches/default/pos</pos>
 </params>
 
 <animation>
  <type>rotate</type>
  <object-name>Pin</object-name>
  <property alias="../../params/pos"/>
  <factor>-30</factor>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>Pin</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-toggle</command>
    <property alias="../../../../params/pos"/>
   </binding>
  </action>
 </animation>
  
</PropertyList>

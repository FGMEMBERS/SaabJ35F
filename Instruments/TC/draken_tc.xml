<?xml version="1.0"?>

<PropertyList>

 <path>draken_tc.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>Needle</object-name>
  <property>/instrumentation/turn-indicator/indicated-turn-rate</property>
  <factor>20.0</factor>
   <center>
    <x-m>0</x-m>
    <y-m>0</y-m>
    <z-m>-0.0184</z-m>
   </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Ball</object-name>
  <property>/instrumentation/slip-skid-ball/indicated-slip-skid</property>
  <factor>5.729</factor>
  <min-deg>-10</min-deg>
  <max-deg>10</max-deg>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0.1</z-m> 
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>



<PropertyList>
 
 <path>draken.ac</path>

 <nasal>
  <load>
     var livery_update = aircraft.livery_update.new("Aircraft/SaabJ35F/Model/Liveries");
  </load> 
  <unload>
     livery_update.stop();
  </unload>
 </nasal>

 <animation>
  <type>material</type>
   <object-name>draken_fuselage</object-name>
   <object-name>draken_elevon_left</object-name>
   <object-name>draken_elevon_right</object-name>
   <object-name>draken_canopy</object-name>
   <object-name>draken_airbrake_left</object-name>
   <object-name>draken_airbrake_right</object-name>
   <object-name>draken_seitenruder</object-name>
   <object-name>draken_quer_links</object-name>
   <object-name>draken_quer_rechts</object-name>
   <object-name>draken_wdf_left</object-name>
   <object-name>draken_wdf_right</object-name>
   <object-name>draken_wdl_left</object-name>
   <object-name>draken_wdl_right</object-name>
   <object-name>draken_wdr_left</object-name>
   <object-name>draken_wdr_right</object-name>
   <property-base>sim/model/livery</property-base>
   <texture-prop>texture</texture-prop>
   <texture>draken_main_sw2.png</texture>
 </animation>

<!-- ********** Instruments indicators and switches ********* -->
 <model>
   <path>Model/instruments.xml</path>
 </model> 
 
 <model>
   <path>Model/switches.xml</path>
 </model>

<!-- ************ Cockpit animations and controls *********** --> 
<!-- Canopy -->
 <animation>
  <type>rotate</type>
  <object-name>draken_canopy</object-name>
  <object-name>draken_canopy_glass02</object-name>
  <object-name>MirrorTop</object-name>
  <object-name>MirrorRight</object-name>
  <object-name>MirrorLeft</object-name>
  <property>/controls/canopy/position-norm</property>
  <factor>55</factor>
  <center>
   <x-m>-2.7165</x-m>
   <y-m>0.0</y-m>
   <z-m>1.2040</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Inst_light_knb</object-name>
  <property>instrumentation/switches/inst-light-knob/pos</property>
  <factor>-200</factor>
  <offset-deg>0</offset-deg>
  <center>
   <x-m>-3.7997</x-m>
   <y-m>-0.40882</y-m>
   <z-m>0.41663</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>
 
 <animation>
  <type>pick</type>
   <object-name>Inst_light_knb</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
      <command>property-cycle</command>
      <property>instrumentation/switches/inst-light-knob/pos</property>
      <value type="double">1</value>
      <value type="double">0.75</value>
      <value type="double">0.50</value>
      <value type="double">0.25</value>
      <value type="double">0</value>
   </binding>
   </action>
 </animation>
  
 <animation>
  <type>material</type> 
  <object-name>draken_instrument</object-name>
  <object-name>draken_cockpit</object-name>
  <condition>		
    <greater-than>		
     <property>/sim/time/sun-angle-rad</property>
     <value>1.57</value>
    </greater-than>
  </condition>
  <emission>	
   <red>0.15</red>	
   <green>0.03</green>	
   <blue>0.03</blue>
   <factor-prop>controls/lighting/instruments-norm</factor-prop>
  </emission>
 </animation>
  
